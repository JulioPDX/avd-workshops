	"service": "avd-workshops",
	"extensions": [
        "aristapublisher.eos",
        "eamodio.gitlens",
        "formulahendry.terminal",
        "github.vscode-pull-request-github",
        "lextudio.restructuredtext",
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "ms-python.python",
